        Title="Vista Previa de Impresión" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="UsuariosDataGrid" AutoGenerateColumns="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
